{
    "name": "ts-compare-fn",
    "version": "1.0.0",
    "description": "",
    "cdn": "dist/index.umd.js",
    "main": "dist/index.js",
    "types": "types/index.d.ts",
    "unpkg": "dist/index.umd.js",
    "module": "dist/index.esm.js",
    "jsdelivr": "dist/index.umd.js",
    "umd:main": "dist/index.umd.js",
    "files": [
        "dist/",
        "types/"
    ],
    "exports": {
        ".": [
            {
                "import": "./dist/index.mjs",
                "require": "./dist/index.cjs",
                "default": "./dist/index.js"
            },
            "./dist/index.js"
        ]
    },
    "devDependencies": {
        "@rollup/plugin-terser": "^0.4.4",
        "@tsd/typescript": "^5.6.2",
        "@types/eslint": "^8.56.12",
        "@types/jest": "^29.5.13",
        "@typescript-eslint/eslint-plugin": "^8.5.0",
        "@typescript-eslint/parser": "^8.5.0",
        "eslint": "^8.57.0",
        "jest": "^29.7.0",
        "jest-runner-tsd": "^6.0.0",
        "prettier": "^3.3.3",
        "rollup": "^4.21.3",
        "rollup-plugin-typescript2": "^0.36.0",
        "ts-jest": "^29.2.5",
        "ts-jest-resolver": "^2.0.1",
        "typescript": "^5.6.2"
    },
    "scripts": {
        "test": "npm run test:code && npm run test:types",
        "test:code": "jest",
        "test:types": "jest --config jest.config.tsd.js",
        "lint": "eslint \"*/**/*.{ts,js,json}\"",
        "lint:fix": "eslint \"*/**/*.{ts,js,json}\" --fix",
        "format": "prettier --write \"./src/**/*.ts\"",
        "format:check": "prettier --check \"./src/**/*.ts\"",
        "build": "rollup --config ./rollup.config.mjs",
        "prepublishOnly": "npm run lint && npm run test && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vmosyaykin/ts-compare-fn.git"
    },
    "keywords": ["compare", "sort", "compare function", "sort function", "array"],
    "author": {
        "name": "Vladimir Mosyaykin"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/vmosyaykin/ts-compare-fn/issues"
    },
    "homepage": "https://github.com/vmosyaykin/ts-compare-fn#readme"
}
